import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.print("Enter your number : ");
		int n =sc.nextInt();
		int reversed =0;
		while(n > 0){
			int lastdigit = n%10;			
			reversed = (reversed * 10)+lastdigit;
			n=n/10;
		}
		System.out.print(reversed);
	}
}
